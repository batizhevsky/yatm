= subscribe_to "/messages/update"
= content_for :users do
  = render "shared/list_users"
%h1 Listing tasks

=link_to new_task_path, class: 'btn btn-primary' do
  %i.icon-plus-sign
  Add task

%table.table#tasks
  %tr
    %th #
    %th Название
    %th Deadline
    %th Назначена
    %th Ответственный

  = render @tasks
%br

